pipeline {
    agent any

    stages {
        stage('one') {
            steps {
                sh echo 'Hello World-stage-1'
            }
        }
        stage('Two') {
            steps {
                sh echo 'Hello World-stage-2'
            }
        }
        stage('Three') {
            steps {
                sh echo 'Hello World-stage-3'
            }
        }
    }
}